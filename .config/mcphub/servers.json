{
  "mcpServers": {
    "github.com/modelcontextprotocol/servers/tree/main/src/filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/shimehituzi/Dropbox/",
        "/Users/shimehituzi/Desktop/"
      ],
      "disabled": false,
      "command": "npx"
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/git": {
      "args": [
        "mcp-server-git"
      ],
      "disabled": false,
      "command": "uvx"
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/time": {
      "args": [
        "mcp-server-time",
        "--local-timezone=Asia/Tokyo"
      ],
      "disabled": false,
      "command": "uvx"
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/github": {
      "args": [
        "-c",
        "GITHUB_PERSONAL_ACCESS_TOKEN=$(gopass show -o github/api_key) npx -y @modelcontextprotocol/server-github"
      ],
      "disabled": false,
      "command": "/bin/zsh"
    },
    "github.com/abhiz123/todoist": {
      "disabled": false,
      "autoApprove": [],
      "args": [
        "-c",
        "TODOIST_API_TOKEN=$(gopass show -o todoist/api_key) npx -y @abhiz123/todoist-mcp-server"
      ],
      "command": "/bin/zsh"
    },
    "github.com/tavily-ai/tavily-mcp": {
      "disabled": false,
      "autoApprove": [],
      "args": [
        "-c",
        "TAVILY_API_KEY=$(gopass show -o tavily/api_key) npx -y tavily-mcp@0.1.4"
      ],
      "command": "/bin/zsh"
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "disabled": false,
      "command": "npx"
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "/Users/shimehituzi/.config/mcphub/memory.json"
      }
    }
  },
  "nativeMCPServers": []
}